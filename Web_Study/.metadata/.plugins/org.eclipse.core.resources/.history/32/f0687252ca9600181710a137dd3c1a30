package dao;

public class ProductDao {
	
	//single-ton pattern: 1개의 객체만 생성해서 서비스하자
	static ProductDao single = null;

	public static ProductDao getInstance() {
		if (single == null)
			single = new ProductDao();
		return single;
	}

	public ProductDao() {
		// TODO Auto-generated constructor stub
	}

}
