<script>
	function find() {

		var addr = $("#addr").val();
		

		if (saname == '') {
			alert("도시를 입력하세요");
			return;
		}
		
		$.ajax({
			url: 'gogek/search.do',
			data: {
				'addr':addr,
			},
			success: function(result){
				$('#disp').html(result);
			}
			
		})		
		

	}
</script>
</head>
<body>

이름입력:
	<input id="addr">
	<input type="button" value="가져오기" onclick="find()">
	<hr>
	<div id="disp"></div>




