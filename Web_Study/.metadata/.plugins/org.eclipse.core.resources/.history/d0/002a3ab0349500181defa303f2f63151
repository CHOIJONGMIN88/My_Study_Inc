package dao;

import org.apache.ibatis.session.SqlSession;

public class DeptDao {
//single-ton pattern: 1개의 객체만 생성해서 서비스하자
static DeptDao single = null;

SqlSession 

public static DeptDao getInstance() {
	if (single == null)
		single = new DeptDao();
	return single;
}

